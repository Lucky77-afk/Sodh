# Copy this file to secrets.toml and fill in your API keys

# Required: Helius API Key for Solana RPC
HELIUS_API_KEY = "your_helius_api_key_here"

# Optional: Other API keys
# OPENAI_API_KEY = "your_openai_api_key_here"

# Database configuration (if needed)
# [database]
# url = "postgresql://user:password@localhost:5432/dbname"
# name = "sodh_db"

# App configuration
[app]
debug = false
log_level = "INFO"

# Feature flags
[features]
enable_experimental = false

# External services
[services]
# solana_network = "mainnet-beta"  # or "devnet" for testing Copy this file to secrets.toml and replace with your actual API key
helius_api_key = "your_helius_api_key_here"
